{
    "version": 2,
    "outputDirectory": "public",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.8.0"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/(.*\\.(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Alba Autocare",
        "APP_ENV": "production",
        "APP_KEY": "base64:TyFcuWcyDx6l4vu9h5FKvFgk5wiPDUfvQkWJe9plY/Q=",
        "APP_DEBUG": "false",
        "APP_URL": "https://alba-autocare.vercel.app",
        "VERCEL": "1",
        "LOG_CHANNEL": "stderr",
        "LOG_LEVEL": "error",
        "SESSION_DRIVER": "cookie",
        "SESSION_LIFETIME": "120",
        "CACHE_STORE": "array",
        "QUEUE_CONNECTION": "sync",
        "DB_CONNECTION": "sqlite",
        "DB_DATABASE": "/tmp/database.sqlite",
        "FILESYSTEM_DISK": "local",
        "BROADCAST_CONNECTION": "log",
        "MAIL_MAILER": "log"
    }
}
