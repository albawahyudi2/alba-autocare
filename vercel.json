{
  "version": 2,
  "buildCommand": "npm run build && php artisan optimize:clear && php artisan config:cache",
  "framework": "laravel",
  "regions": ["sin1"],
  "env": {
    "APP_NAME": {
      "description": "Application Name"
    },
    "APP_KEY": {
      "description": "Application Encryption Key"
    },
    "APP_ENV": {
      "value": "production"
    },
    "APP_DEBUG": {
      "value": "false"
    },
    "LOG_CHANNEL": {
      "value": "stderr"
    },
    "LOG_LEVEL": {
      "value": "error"
    },
    "DB_CONNECTION": {
      "value": "pgsql"
    },
    "DB_HOST": {
      "description": "Supabase Host URL"
    },
    "DB_PORT": {
      "value": "5432"
    },
    "DB_DATABASE": {
      "description": "Supabase Database Name"
    },
    "DB_USERNAME": {
      "description": "Supabase Username"
    },
    "DB_PASSWORD": {
      "description": "Supabase Password"
    },
    "SESSION_DRIVER": {
      "value": "file"
    },
    "QUEUE_CONNECTION": {
      "value": "sync"
    },
    "CACHE_STORE": {
      "value": "array"
    }
  },
  "functions": {
    "api/index.php": {
      "runtime": "php-8.3"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.php"
    }
  ]
}
